{
  "name": "plain-ioc",
  "version": "1.1.0",
  "description": "Plain inversion of control container",
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "npm run clean && node ./node_modules/.bin/tsc",
    "clean": "node -e \"['./dist'].forEach(item => require('fs').rmSync(item, {recursive:true,force:true}));\"",
    "prepack": "npm run build"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": ">=18.0.0",
      "onFail": "error"
    },
    "packageManager": {
      "name": "npm",
      "version": ">=8.6.0",
      "onFail": "error"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mvcbox/node-plain-ioc.git"
  },
  "keywords": [
    "inversion",
    "of",
    "control",
    "container"
  ],
  "author": "mvcbox.org@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mvcbox/node-plain-ioc/issues"
  },
  "homepage": "https://github.com/mvcbox/node-plain-ioc#readme",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@types/node": "^6.14.13",
    "typescript": "^4.7.2"
  }
}
